{
  "name": "api",
  "version": "0.0.0",
  "scripts": {
    "clean-install": "rm -rf ./node_modules && rm -r package-lock.json && npm i",
    "dev:hot": "nodemon --exec \"npm run dev\" --watch ./src --ext .ts",
    "dev": "cross-env PORT=5000 NODE_ENV=development ts-node-dev --respawn --transpile-only src/server.ts",
    "lint": "eslint .",
    "lint-staged": {
      "*.ts": "eslint --fix"
    },
    "start": "NODE_ENV=production node -r ./config.js ./dist",
    "test": "NODE_ENV=test vitest",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "seed": "cross-env NODE_ENV=development ts-node src/seed/seed.ts",
    "build": "tsc"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^17.0.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "fast-csv": "^5.0.2",
    "helmet": "^8.1.0",
    "inserturlparams": "^2.0.5",
    "jet-env": "^1.1.4",
    "jet-logger": "^2.0.1",
    "jet-paths": "^1.1.0",
    "jet-validators": "^1.4.3",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mongoose": "^8.16.1",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@stylistic/eslint-plugin-js": "^4.4.1",
    "@stylistic/eslint-plugin-ts": "^4.4.1",
    "@swc/core": "^1.12.7",
    "@types/bcryptjs": "^3.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/find": "^0.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonfile": "^6.1.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/module-alias": "^2.0.4",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.7",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "cross-env": "^7.0.3",
    "eslint": "^9.30.0",
    "eslint-plugin-n": "^17.20.0",
    "find": "^0.3.0",
    "fs-extra": "^11.3.0",
    "husky": "^8.0.0",
    "jest": "^30.0.3",
    "jiti": "^2.4.2",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "vitest": "^3.2.4"
  }
}
